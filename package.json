{
  "name": "kirby-starterkit",
  "version": "1.0.0",
  "description": "Starterkit for Kirby 3",
  "author": "sinanatra",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/sinanatra/kirby-starterkit#readme",
  "bugs": {
    "url": "https://github.com/sinanatra/kirby-starterkit/issues"
  },
  "scripts": {
    "server": "APP_ENV=dev php -S localhost:3000 -t www www/kirby/router.php",
    "dev": "webpack --mode=development --watch",
    "build": "rm -rf ./www/assets && webpack --mode=production",
    "start": "concurrently \"npm:dev\" \"npm:server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinanatra/kirby-starterkit.git"
  },
  "dependencies": {
    "babel-plugin-lodash": "^3.3.4",
    "jquery": "^3.5.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "modernizr-webpack-plugin": "^0.0.2",
    "open-cli": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "sanitize.css": "^11.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-livereload-plugin": "^2.3.0",
    "webpack-merge": "^4.2.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "cssnano": {}
    }
  }
}
